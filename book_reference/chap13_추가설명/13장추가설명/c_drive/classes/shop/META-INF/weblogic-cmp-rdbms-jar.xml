<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>
<weblogic-rdbms-jar>
    <weblogic-rdbms-bean>
        <ejb-name>Category</ejb-name>
        <data-source-name>ora9</data-source-name>
        <table-map>
            <table-name>CATEGORY</table-name>
            <field-map>
                <cmp-field>name</cmp-field>
                <dbms-column>NAME</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>text</cmp-field>
                <dbms-column>TEXT</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
    <weblogic-rdbms-bean>
        <ejb-name>Item</ejb-name>
        <data-source-name>ora9</data-source-name>
        <table-map>
            <table-name>ITEM</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>ID</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>categoryname</cmp-field>
                <dbms-column>CATEGORYNAME</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>name</cmp-field>
                <dbms-column>NAME</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>price</cmp-field>
                <dbms-column>PRICE</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>text</cmp-field>
                <dbms-column>TEXT</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>regdate</cmp-field>
                <dbms-column>REGDATE</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
    <weblogic-rdbms-bean>
        <ejb-name>Users</ejb-name>
        <data-source-name>ora9</data-source-name>
        <table-map>
            <table-name>USERS</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>ID</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>name</cmp-field>
                <dbms-column>NAME</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>passwd</cmp-field>
                <dbms-column>PASSWD</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>userlevel</cmp-field>
                <dbms-column>USERLEVEL</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
</weblogic-rdbms-jar>
