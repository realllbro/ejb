<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-rdbms-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN' 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>
<weblogic-rdbms-jar>
    <weblogic-rdbms-bean>
        <ejb-name>Customer</ejb-name>
        <data-source-name>ora9</data-source-name>
        <table-map>
            <table-name>CUSTOMER</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>ID</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>password</cmp-field>
                <dbms-column>PASSWORD</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>name</cmp-field>
                <dbms-column>NAME</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
    <weblogic-rdbms-bean>
        <ejb-name>Interestitem</ejb-name>
        <data-source-name>ora9</data-source-name>
        <table-map>
            <table-name>INTERESTITEM</table-name>
            <field-map>
                <cmp-field>itemid</cmp-field>
                <dbms-column>ITEMID</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>itemname</cmp-field>
                <dbms-column>ITEMNAME</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
    <weblogic-rdbms-bean>
        <ejb-name>Selectitem</ejb-name>
        <data-source-name>ora9</data-source-name>
        <table-map>
            <table-name>SELECTITEM</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>ID</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>itemid</cmp-field>
                <dbms-column>ITEMID</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>itemname</cmp-field>
                <dbms-column>ITEMNAME</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
    <weblogic-rdbms-bean>
        <ejb-name>Moreinfo</ejb-name>
        <data-source-name>ora9</data-source-name>
        <table-map>
            <table-name>MOREINFO</table-name>
            <field-map>
                <cmp-field>id</cmp-field>
                <dbms-column>ID</dbms-column>
            </field-map>
            <field-map>
                <cmp-field>point</cmp-field>
                <dbms-column>POINT</dbms-column>
            </field-map>
        </table-map>
    </weblogic-rdbms-bean>
    <weblogic-rdbms-relation>
        <relation-name>customer-selectitem</relation-name>
        <weblogic-relationship-role>
            <relationship-role-name>SelectitemRelationshipRole</relationship-role-name>
            <relationship-role-map>
                <primary-key-table>CUSTOMER</primary-key-table>
                <column-map>
                    <foreign-key-column>ID</foreign-key-column>
                    <key-column>ID</key-column>
                </column-map>
            </relationship-role-map>
        </weblogic-relationship-role>
    </weblogic-rdbms-relation>
    <weblogic-rdbms-relation>
        <relation-name>customer-moreinfo</relation-name>
        <weblogic-relationship-role>
            <relationship-role-name>MoreinfoRelationshipRole</relationship-role-name>
            <relationship-role-map>
                <foreign-key-table>MOREINFO</foreign-key-table>
                <primary-key-table>CUSTOMER</primary-key-table>
                <column-map>
                    <foreign-key-column>ID</foreign-key-column>
                    <key-column>ID</key-column>
                </column-map>
            </relationship-role-map>
        </weblogic-relationship-role>
    </weblogic-rdbms-relation>
    <weblogic-rdbms-relation>
        <relation-name>customer-interestitem</relation-name>
        <table-name>CUSTOMER_INTERESTITEM</table-name>
        <weblogic-relationship-role>
            <relationship-role-name>CustomerRelationshipRole</relationship-role-name>
            <relationship-role-map>
                <primary-key-table>CUSTOMER</primary-key-table>
                <column-map>
                    <foreign-key-column>ID</foreign-key-column>
                    <key-column>ID</key-column>
                </column-map>
            </relationship-role-map>
        </weblogic-relationship-role>
        <weblogic-relationship-role>
            <relationship-role-name>InterestitemRelationshipRole</relationship-role-name>
            <relationship-role-map>
                <primary-key-table>INTERESTITEM</primary-key-table>
                <column-map>
                    <foreign-key-column>ITEMID</foreign-key-column>
                    <key-column>ITEMID</key-column>
                </column-map>
            </relationship-role-map>
        </weblogic-relationship-role>
    </weblogic-rdbms-relation>
</weblogic-rdbms-jar>
